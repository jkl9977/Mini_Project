CREATE TABLE members (
		member_num VARCHAR2(20),
    member_id VARCHAR2(50),
    member_name VARCHAR2(100) ,
    member_email VARCHAR2(150),
    member_password VARCHAR2(100),
    member_address VARCHAR2(200),
    member_address_detail VARCHAR2(200),
    member_postcode VARCHAR2(20),
    role VARCHAR2(50),
    member_phone1 VARCHAR2(15),
    member_phone2 VARCHAR2(15),
    member_join_date DATE,
    member_profile_image VARCHAR(255),
    member_birth DATE
    
);
